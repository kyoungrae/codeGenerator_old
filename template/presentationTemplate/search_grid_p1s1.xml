<script type="text/javascript">
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// variables
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
	
var PRGMID = "html#Service#";	//page id 

//grid variable
var __ROWCOUNT_PER_PAGE__ = 10;  //grid rowcount
var #service#_Grid = null; //grid instance
var #service#_Paging = null; //paging instance

var updateSupportCodeLock = false; //update lock 
	
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// init functions
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

/**
 * main onload
 */
WatDocument.onload = function() {

	WatTop.documentDivResize( #service#_resize );

	initEvts();
	initGrid();
	initSelect();
	initDefaultValue();
	periodBtnSet();
	refreshCtrlBts();

	WatJs.Common.setTimeout(0.5,function(){
		window.onresize();
		search#Service#();
	});
	
};
	
/**
 * 톱니 눌렀을 때 기간 간편설정 창 슬라이드
 */
var periodBtn=0;
$(".dl-period-search-btn").hide();
$(".dl-period-search-btn-small").click(function(){
	if(0==periodBtn){
		$(".dl-period-search-btn-small span i").css({"transform":"rotate(45deg)"});
		$(".dl-period-search-btn").slideDown(200,"linear");
		$(".dl-period-search-btn-small span").attr("title","기간간편설정 닫기");
		periodBtn=1;
	} else{
		initDefaultPeriodPop();
	}
});
$(".dl-period-search-btn .closeBtn").click(function(){
	initDefaultPeriodPop();
});

/**
 * 전체 기간 조회 버튼 클릭 이벤트
 */
var totPeriodChk=false;
$("#totPeriod").click(function(){
	totPeriodChk=$(this).prop("checked");
	if(totPeriodChk){
		WatJs.Common.setControlValue("date_from", "");
		WatJs.Common.setControlValue("date_to", "");
		$("#date_from, #date_to, .dl-period-search-btn button").css({"pointerEvents":"none"});
		$("#date_from, #date_to, .dl-period-search-btn button").attr("readonly","");
	} else{
		WatJs.Common.setControlValue("date_from", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisMonthFirstday()));
		WatJs.Common.setControlValue("date_to", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getToday()));
		$("#date_from, #date_to, .dl-period-search-btn button").css({"pointerEvents":"auto"});
		$("#date_from, #date_to, .dl-period-search-btn button").removeAttr("readonly");
	}
});

/**
 * periodPop init button setting
 */
function periodBtnSet(){
	$(".btn-period").click(function(){
		var periodId=$(this).val();
		//console.log(periodId);
		if("today"==periodId){
			WatJs.Common.setControlValue("date_from", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getToday()));
			WatJs.Common.setControlValue("date_to", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getToday()));
		} else if("thisweek"==periodId){
			WatJs.Common.setControlValue("date_from", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisWeekFirstday()));
			WatJs.Common.setControlValue("date_to", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisWeekLastday()));
		} else if("thismonth"==periodId){
			WatJs.Common.setControlValue("date_from", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisMonthFirstday()));
			WatJs.Common.setControlValue("date_to", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisMonthLastday()));
		} else if("thisyear"==periodId){
			WatJs.Common.setControlValue("date_from", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisYearFirstday()));
			WatJs.Common.setControlValue("date_to", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisYearLastday()));
		}
	});
}

/**
 * periodPop init setting
 */
function initDefaultPeriodPop(){
	$(".dl-period-search-btn-small span i").css({"transform":"rotate(0)"});
	$(".dl-period-search-btn").slideUp(200,"linear");
	$(".dl-period-search-btn-small span").attr("title","기간간편설정 열기");
	periodBtn=0;
}

/**
 * total period init setting
 */
function initDefaultTotPeriod(){
	totPeriodChk=false;
	$("#totPeriod").prop("checked",false);
	$("#date_from, #date_to, .dl-period-search-btn button").css({"pointerEvents":"auto"});
	$("#date_from, #date_to, .dl-period-search-btn button").removeAttr("readonly");
}

/**
 * datepicker&input init setting
 */
function initDefaultValue(){
	WatJs.Common.setControlValue("date_from", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getThisMonthFirstday()));
	WatJs.Common.setControlValue("date_to", WatJs.Util.Date.getFormatDate.yyyy_mm_dd(WatJs.Util.Date.getToday()));
	WatJs.Common.setControlValue("#service#_Desc","");
}

/**
 * row count select init
 */
function initSelect(){
	 //row카운트 추가 부분
	var rowCountSelect=new WatJs.Custom.CodeSelect("showNoGrid","GRID_ROW_COUNT");
	rowCountSelect.setEventOnChange(function(val,text){				
		if(0==val)return;
		search#Service#();
	});	
	rowCountSelect.visibleTitle(true);
	rowCountSelect.init();
}

/**
 * init event
 */
function initEvts() {
	
	// excel button click event
	WatJs.Common.setEvt(PRGMID, "excel-click", "click", function(evt) {		
		#service#_Grid.toExcel();
	});	
	
	// search button click event
	WatJs.Common.setEvt(PRGMID,"search-click","click",function(evt){
		initDefaultPeriodPop();
		search#Service#();
	});

	// reset button click event
	WatJs.Common.setEvt(PRGMID, "default-click", "click", function(evt) {
		initDefaultValue();
		initDefaultPeriodPop();
		initDefaultTotPeriod();
		search#Service#();
	});
	
	// view button click event
	WatJs.Common.setEvt(PRGMID,"popup-View_Popup-click","click",function(evt){
		var rowId = #service#_Grid.getSelectedRowId();
		popupView#Service#(rowId);
	});
	
	// add button click event
	WatJs.Common.setEvt(PRGMID,"popup-Add_Popup-click","click",function(evt){
		popupAdd#Service#();
	});
	
	// modify button click event
	WatJs.Common.setEvt(PRGMID,"popup-Modify_Popup-click","click",function(evt){
		var rowId = #service#_Grid.getSelectedRowId();
		popupModify#Service#(rowId);
	});
	
	// drop button click event
	WatJs.Common.setEvt(PRGMID,"popup-Drop_Popup-click","click",function(evt){
		var rowId = #service#_Grid.getSelectedRowId();
		popupDrop#Service#(rowId);
	});

	// add popup - save button click event
	WatJs.Common.setEvt(PRGMID,"save-Add_Popup-click","click",function(evt){
		if (!isValidAdd#Service#())
			return;
		
		updateSupportCodeLock = true;
		var content = WatJs.Common.getFormValues("Add_PopupForm");
		updateSupportCodeLock = false;
		add#Service#(content);
		
		WatJs.Common.closePopup();
	});
	
	// modify popup - save button click event
	WatJs.Common.setEvt(PRGMID,"save-Modify_Popup-click","click",function(evt){
		if (!isValidModify#Service#())
			return;
		
		updateSupportCodeLock = true;
		var content = WatJs.Common.getFormValues("Modify_PopupForm");
		updateSupportCodeLock = false;
		modify#Service#(content);
		
		WatJs.Common.closePopup();
	});
	
	// drop popup - save button click event
	WatJs.Common.setEvt(PRGMID,"save-Drop_Popup-click","click",function(evt){
		if (!isValidDrop#Service#())
			return;
		
		updateSupportCodeLock = true;
		var content = WatJs.Common.getFormValues("Drop_PopupForm");
		updateSupportCodeLock = false;
		drop#Service#(content);
		
		WatJs.Common.closePopup();
	});
	
	// popup close button click event - view close
	WatJs.Common.setEvt(PRGMID,"close-popup-click","click",function(evt){
		WatJs.Common.closePopup();
	});
	
	// popup close button click event - add cancel
	WatJs.Common.setEvt(PRGMID,"close-Add_Popup-click","click",function(evt){
		WatJs.Common.closePopup();
	});
	
	// popup close button click event - modify cancel
	WatJs.Common.setEvt(PRGMID,"close-Modify_Popup-click","click",function(evt){
		WatJs.Common.closePopup();
	});
	
	// popup close button click event - drop cancel
	WatJs.Common.setEvt(PRGMID,"close-Drop_Popup-click","click",function(evt){
		WatJs.Common.closePopup();
	});
}
	
/**
 * main resize event handler
 */
function #service#_resize(wid,hei) {
	var serHei = hei;
	if (#service#_Grid) {
		$("##service#_Grid").css("height","auto");
		$("##service#_Grid objbox").css("height","auto");
	}
}

/**
 * grid & paging initialize 
 *
 * 항목별 크기
 * No. -> 50
 * 날짜 -> 150
 * 사용자 -> 100
 */
function initGrid() {
	try {
		
		//grid 
		#service#_Grid = new WatJs.Component.Grid("#service#_Grid");
		#service#_Grid.setHeader([ 
			//{"KEY":"NO",		"NAME":"No.",										"WIDTH":"50",		"ALIGN" : "center",	"SORT" : "int", "TYPE" : "ro"},#begin#
			{"KEY" : "#field_name#",	"NAME" : WatJs.Label.Array["#scope#.#service#.#field_name#"],	"WIDTH" : "*",	"ALIGN" : "center",	"SORT" : "str",	"TYPE" : "ro"}#conjunction##end#
